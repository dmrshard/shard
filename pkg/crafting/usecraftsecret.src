program CraftScroll(who, Scroll)

  if(!StandardItemCheck(who, Scroll, NO_BUSY_CHECK))
    return;
  endif

  SendGood(who, "Zanies ten zwoj do jednego z rzemieslnikow. Oni sa w stanie zidentyfikowac tajniki.");

endprogram

